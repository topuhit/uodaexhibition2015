<script type="text/javascript">
	    var cl = cloudinary.Cloudinary.new({cloud_name: "topuhit"}); 
    // replace 'demo' with your cloud name in the line above 
    cl.responsive();






    jQuery(document).ready(function() {
     $("time.timeago").timeago();
     $("time.timeago.title").timeago();



var a = window.location.href

var desired = a.replace(/https\:\/\/uodaexhibition2015.herokuapp.com\//gi, '')
console.log(desired);


axios.get('https://uodaexhibition2015.herokuapp.com/api/'+ desired)
  .then(function (response) {
    document.getElementById("viewscount").innerHTML = response.data.count;
  })
  .catch(function (error) {
    console.log(error);
  });


   });



	
</script>

<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script type="text/javascript">

axios.get('https://uodaexhibition2015.herokuapp.com/related/'+ <% if (batch) { %><%= batch %><% } %>)
  .then(function (response) {
   

    for (var i = 0, len = response.data.length; i < len; i++) {


$("#related").append("<div class=\"marginauot\"><a href=\"https://uodaexhibition2015.herokuapp.com/" + response.data[i].seourl + "\"><img src=\"https://a2.res.cloudinary.com/topuhit/image/upload/w_200,h_200,c_thumb/" + response.data[i].artwork.public_id + "." + response.data[i].artwork.format + "\" srcset=\"https://a2.res.cloudinary.com/topuhit/image/upload/w_200,h_200,c_thumb/" + response.data[i].artwork.public_id + "." + response.data[i].artwork.format + " 1x, https://a2.res.cloudinary.com/topuhit/image/upload/w_400,h_400,c_thumb/" + response.data[i].artwork.public_id + "." + response.data[i].artwork.format + " 2x\"><br><p class=\"similar\">" + response.data[i].name + "</p><p class=\"similar2\">" + response.data[i].batch + "</p></div></a>");

}


  })
  .catch(function (error) {
    console.log(error);
  });


var nextbatch = <% if (batch) { %><%= batch %><% } %>

nextbatch = nextbatch +1

axios.get('https://uodaexhibition2015.herokuapp.com/nextrelated/'+ nextbatch)
  .then(function (response) {
    

    for (var i = 0, len = response.data.length; i < len; i++) {




$("#nextrelated").append("<div class=\"marginauot\"><a href=\"https://uodaexhibition2015.herokuapp.com/" + response.data[i].seourl + "\">\n<img src=\"https://a2.res.cloudinary.com/topuhit/image/upload/w_200,h_200,c_thumb/" + response.data[i].artwork.public_id + "." + response.data[i].artwork.format + "\"  srcset=\"https://a2.res.cloudinary.com/topuhit/image/upload/w_200,h_200,c_thumb/" + response.data[i].artwork.public_id + "." + response.data[i].artwork.format + " 1x, https://a2.res.cloudinary.com/topuhit/image/upload/w_400,h_400,c_thumb/" + response.data[i].artwork.public_id + "." + response.data[i].artwork.format + " 2x\"><br>\n<p class=\"similar\">" + response.data[i].name + "</p>\n<p class=\"similar2\">" + response.data[i].batch + "</p>\n</a>\n</div>");




}


  })
  .catch(function (error) {
    console.log(error);
  });



</script>

