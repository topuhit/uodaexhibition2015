    <style>
    body {
        width: 70%;
        margin: 0 auto;
        font-family: monospace;
        font-size: 24px;
    }

    .col-xs-3 {
        width: 30%
    }

    table {
    counter-reset: rowNumber;
}

table tr {
    counter-increment: rowNumber;
}

table tr td:first-child::before {
    content: counter(rowNumber);
    min-width: 1em;
    margin-right: 2em;
}


    </style>
</head>

<body>





<div class="w3-container">
  <h2>Basic Table</h2>
  <p>The w3-table class defines a basic table:</p>

  <table class="w3-table">
    <tr>

      <th>First Name</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>BFA</th>
      <th>Batch</th>
    </tr>

         <% speakers.forEach(function(item) { %>
    <tr>

      <td><%= item.name%></td>
      <td><img src="http://res.cloudinary.com/topuhit/image/upload/w_400,c_scale/v1509655299/exhibition/award-02_03-20171103024044_jpyzjy.jpg" style="height: 50px;"></td>
      <td><%= item.title%></td>
      <td><%= item.bfa%><%= item.mfa%></td>
      <td><%= item.batch%></td>
    </tr>
         <%  }); %>
  </table>
</div>







    
